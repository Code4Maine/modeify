#!/bin/sh

cd "$(dirname "$0")"

BUCKET=`./config-val $1 s3_bucket`

echo "Building client assets..."
./build-client $1

echo "Syncing assets to $BUCKET"
aws s3 sync ../assets $BUCKET \
  --acl public-read

echo "Syncing config to $BUCKET"
aws s3 cp ../config/config.yaml $BUCKET \
  --acl public-read
